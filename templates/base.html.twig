	<body class="text-gray-800 dark:text-gray-200">
		<div class="min-h-screen bg-gray-100 dark:bg-gray-900 pt-24">
			<header x-data="{navbarOpen: false}" class="fixed left-0 top-0 z-50 bg-white w-full flex items-center shadow-md dark:bg-slate-900 h-24">
				<div class="container">
					<div class="flex -mx-4 items-center justify-between relative">
						<div class="pr-4 w-60 max-w-full">
							<a href="#" class="w-full flex items-center py-2">
								logo
								<span class="text-xl xl:text-2xl font-bold text-[#0c7187] dark:text-white">name</span>
							</a>
						</div>
						<div class="flex px-4 justify-end items-center w-full">
							<div>
								<button id="navbarToggler" class="block absolute right-4 top-1/2 -translate-y-1/2 lg:hidden focus:ring-2 ring-primary px-3 py-[6px] rounded-lg">
									<span class="relative w-[30px] h-[2px] my-[6px] block bg-body-color"></span>
									<span class="relative w-[30px] h-[2px] my-[6px] block bg-body-color"></span>
									<span class="relative w-[30px] h-[2px] my-[6px] block bg-body-color"></span>
								</button>
								<nav :class="!navbarOpen && 'hidden' " id="navbarCollapse" class="absolute right-0 top-full bg-white py-5 px-6 z-50 shadow rounded-lg w-full dark:bg-slate-900 dark:text-gray-300 lg:px-0 lg:max-w-full lg:w-full lg:right-4 lg:block lg:static lg:shadow-none">
									<ul class="block lg:flex lg:items-center">
										<li>
											<a href="#" class="text-xs font-medium text-dark py-2 flex hover:text-primary dark:text-gray-200 lg:inline-flex lg:ml-6 xl:ml-12">
												About
											</a>
										</li>
										<li>
											<a href="#" class="text-xs font-medium text-dark py-2 flex hover:text-primary dark:text-gray-200 lg:inline-flex lg:ml-6 xl:ml-12">
												Projects
											</a>
										</li>
										<li>
											<a href="#" class="text-xs font-medium text-dark py-2 flex hover:text-primary dark:text-gray-200 lg:inline-flex lg:ml-6 xl:ml-12">
												Testimonials
											</a>
										</li>
									</ul>
								</nav>
							</div>
						</div>
					</div>
				</div>
			</header>

